{
	"name": "cache",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSql_customer",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "cache_source",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "surrogateKey"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          maxCustomerID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select max(CustomerID)  as maxCustomerID from SalesLT.Customer',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          Name as string,",
				"          ProductNumber as string,",
				"          Color as string,",
				"          StandardCost as string,",
				"          ListPrice as integer,",
				"          Size as integer,",
				"          Weight as integer,",
				"          ProductCategoryID as integer,",
				"          ProductModelID as integer,",
				"          SellStartDate as string,",
				"          SellEndDate as string,",
				"          DiscontinuedDate as string,",
				"          ThumbNailPhoto as string,",
				"          ThumbnailPhotoFileName as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"surrogateKey derive(CustomerID = toInteger(CustomerID)) ~> derivedColumn1",
				"source2 keyGenerate(output(CustomerID as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey",
				"derivedColumn1 derive(CustomerID = ERROR_FUNCTION('sink1#outputs().[1].maxCustomerID')) ~> derivedColumn2",
				"source1 sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1,",
				"     mapColumn(",
				"          maxCustomerID",
				"     )) ~> sink1"
			]
		}
	}
}